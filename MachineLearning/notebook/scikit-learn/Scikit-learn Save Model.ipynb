   "source": [
    "from sklearn import svm\n",
    "from sklearn import datasets\n",
    "clf = svm.SVC()\n",
    "iris = datasets.load_iris()\n",
    "X,y=iris.data,iris.target\n",
    "clf.fit(X,y)\n",
    "\n",
    "import pickle\n",
    "with open('save/clf.pickle','wb') as f: # rb\n",
    "    pickle.dump(clf,f) # load"
   ]
  }
